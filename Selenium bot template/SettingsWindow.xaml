<Window x:Class="Selenium_bot_template.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Selenium_bot_template"
        mc:Ignorable="d"
        Title="Settings" Height="300" Width="300"
        Style ="{StaticResource {x:Type Window}}">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Images/darkepic.jpg"/>
        </Grid.Background>
        <Button x:Name="CloseButton" Content="Close" HorizontalAlignment="Left" Margin="207,240,0,0" VerticalAlignment="Top" Width="75" Click="CloseButton_Click"/>
    </Grid>
</Window>
